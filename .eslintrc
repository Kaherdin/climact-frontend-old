{
  "extends": ["next", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "printWidth": 80,
        "singleQuote": false,
        "trailingComma": "es5",
        "semi": true,
        "endOfLine": "auto",
        "tabWidth": 2
      }
    ],
    "@next/next/no-document-import-in-page": "off"
  }
}
